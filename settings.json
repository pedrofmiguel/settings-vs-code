{
  "sync.gist": "0fc2006a3f8373d603128d5d0f132579",
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "tabnine.experimentalAutoImports": true,
  // editor Settings
  "editor.fontFamily": "Jetbrains mono",
  "editor.fontWeight": 300,
  "editor.fontLigatures": true,
  "editor.fontSize": 15.5,
  "editor.minimap.renderCharacters": false,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.lineHeight": 2,
  "editor.guides.bracketPairs": "active",
  "editor.minimap.showSlider": "always",
  "editor.showFoldingControls": "always",
  "editor.minimap.maxColumn": 80,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.formatOnPaste": true,
  "editor.tabSize": 2,
  "editor.cursorBlinking": "smooth",
  "editor.bracketPairColorization.enabled": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },

  // Workbench settings
  "workbench.tree.ident": 20,
  "workbench.list.smoothScrolling": true,
  "workbench.iconTheme": "gruvbox-material-icon-theme",
  "workbench.colorTheme": "Everforest Dark",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ff9966",
    "editorCursor.background": "#000000",
    "terminalCursor.foreground": "#66ff99"
  },
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "always",
  //"workbench.activityBar.visible": false,
  //"workbench.layoutControl.enabled": false,

  // customize UI
  "customizeUI.activityBarHideSettings": true,
  "customizeUI.listRowHeight": 30,
  "customizeUI.titleBar": "inline",

  // languages settings
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  //eslint stuff
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "extensinons": [".ts", ".js", ".tsx", ".jsx"]
  },

  // Git
  "git.mergeEditor": false,
  "git.confirmSync": false,

  // Terminal
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.defaultProfile.osx": "zsh",

  // Prettier
  "prettier.printWidth": 80,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,

  // Errors
  "errorLens.delay": 200,
  "errorLens.gutterIconSize": "80%",
  "errorLens.messageBackgroundMode": "message",

  // diff editor
  "diffEditor.codeLens": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,

  // Misc
  "explorer.confirmDelete": false,
  "security.workspace.trust.untrustedFiles": "open",
  "json.schemas": [],
  "window.zoomLevel": 1,
  "settingsSync.ignoredSettings": []
}
